{
  "type": "module",
  "name": "with-riot",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "test": "nyc --require esm --require jsdom-global/register --require @riotjs/register mocha src/**/*.spec.js",
    "cov": "nyc report --reporter=text-lcov",
    "cov-html": "nyc report --reporter=html",
    "build": "rollup -c",
    "prepublishOnly": "npm test",
    "start": "npm run dev & npm run server",
    "watch": "rollup -c -w 'src/**'",
    "server": "browser-sync start --startPath dist/ --files 'dist/*' --server",
    "dev": "tsc --watch & rollup -c -w",
    "format": "prettier --write \"**/*.{ts,riot}\""
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@capacitor/cli": "^5.5.0",
    "@riotjs/compiler": "^9.0.6",
    "@riotjs/register": "^9.0.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/chai": "^4.3.9",
    "@types/mocha": "^10.0.3",
    "browser-sync": "^2.27.10",
    "chai": "^4.3.6",
    "esm": "^3.2.25",
    "jsdom": "^20.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "riot": "^9.1.1",
    "rollup": "^4.1.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-bundle": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-riot": "^9.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@capacitor/android": "^5.5.0",
    "@capacitor/core": "^5.5.0",
    "@picocss/pico": "^1.5.10",
    "riot": "^9.1.1",
    "valtio": "^1.11.2"
  }
}
